{
    "Bootstrap 5 List Group": {
		"prefix": "b5-list-group",
		"body": [
			"<ul class=\"list-group\">",
				"\t<li class=\"list-group-item active\" aria-current=\"true\">An active item</li>",
				"\t<li class=\"list-group-item\">A second item</li>",
				"\t<li class=\"list-group-item\">A third item</li>",
				"\t<li class=\"list-group-item\">A fourth item</li>",
				"\t<li class=\"list-group-item disabled\" aria-disabled=\"true\">And a fifth one</li>",
			"</ul>"
		],
		"description": "b5-list-group"
	},

	"Bootstrap 5 List Link": {
		"prefix": "b5-list-link",
		"body": [
			"<div class=\"list-group\">",
				"\t<a href=\"#\" class=\"list-group-item list-group-item-action active\" aria-current=\"true\">",
					"\t\tThe current link item",
				"\t</a>",
				"\t<a href=\"#\" class=\"list-group-item list-group-item-action\">A second link item</a>",
				"\t<a href=\"#\" class=\"list-group-item list-group-item-action\">A third link item</a>",
				"\t<a href=\"#\" class=\"list-group-item list-group-item-action\">A fourth link item</a>",
				"\t<a href=\"#\" class=\"list-group-item list-group-item-action disabled\" tabindex=\"-1\" aria-disabled=\"true\">A disabled link item</a>",
			"</div>"
		],
		"description": "b5-list-link"
	},

