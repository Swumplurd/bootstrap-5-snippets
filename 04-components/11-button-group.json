			"</div>"
		],
		"description": "b5-button-group"
	},

	"Bootstrap 5 Checkbox Button Group": {
		"prefix": "b5-checkbox-button-group",
		"body": [
			"<div class=\"btn-group\" role=\"group\" aria-label=\"Basic checkbox toggle button group\">",
				"\t<input type=\"checkbox\" class=\"btn-check\" id=\"btncheck1\" autocomplete=\"off\">",
				"\t<label class=\"btn btn-outline-primary\" for=\"btncheck1\">Checkbox 1</label>",

				"\n\t<input type=\"checkbox\" class=\"btn-check\" id=\"btncheck2\" autocomplete=\"off\">",
				"\t<label class=\"btn btn-outline-primary\" for=\"btncheck2\">Checkbox 2</label>",

				"\n\t<input type=\"checkbox\" class=\"btn-check\" id=\"btncheck3\" autocomplete=\"off\">",
				"\t<label class=\"btn btn-outline-primary\" for=\"btncheck3\">Checkbox 3</label>",
			"</div>"
		],
		"description": "b5-checkbox-button-group"
	},

	"Bootstrap 5 Radio Button Group": {
		"prefix": "b5-radio-button-group",
		"body": [
			"<div class=\"btn-group\" role=\"group\" aria-label=\"Basic radio toggle button group\">",
				"\t<input type=\"radio\" class=\"btn-check\" name=\"btnradio\" id=\"btnradio1\" autocomplete=\"off\" checked>",
				"\t<label class=\"btn btn-outline-primary\" for=\"btnradio1\">Radio 1</label>",

				"\n\t<input type=\"radio\" class=\"btn-check\" name=\"btnradio\" id=\"btnradio2\" autocomplete=\"off\">",
				"\t<label class=\"btn btn-outline-primary\" for=\"btnradio2\">Radio 2</label>",

				"\n\t<input type=\"radio\" class=\"btn-check\" name=\"btnradio\" id=\"btnradio3\" autocomplete=\"off\">",
				"\t<label class=\"btn btn-outline-primary\" for=\"btnradio3\">Radio 3</label>",
			"</div>"
		],
		"description": "b5-radio-button-group"
	}
}