{
    "Bootstrap 5 Progress": {
		"prefix": "b5-progress",
		"body": [
			"<div class=\"progress\">",
				"\t<div class=\"progress-bar bg-${2|primary,secondary,success,danger,warning,info,light,dark|} ${3|progress-bar-striped, |} ${4|progress-bar-animated, |}\" role=\"progressbar\" style=\"width: ${1:25}%;\" aria-valuenow=\"${1:25}\" aria-valuemin=\"0\" aria-valuemax=\"100\">${1:25}%</div>",
			"</div>"
		],
		"description": "b5-progress"
	},

	"Bootstrap 5 Progress Multiple": {
		"prefix": "b5-progress-multiple",
		"body": [
			"<div class=\"progress\">",
				"\t<div class=\"progress-bar\" role=\"progressbar\" style=\"width: 15%\" aria-valuenow=\"15\" aria-valuemin=\"0\" aria-valuemax=\"100\"></div>",
				"\t<div class=\"progress-bar bg-success\" role=\"progressbar\" style=\"width: 30%\" aria-valuenow=\"30\" aria-valuemin=\"0\" aria-valuemax=\"100\"></div>",
				"\t<div class=\"progress-bar bg-info\" role=\"progressbar\" style=\"width: 20%\" aria-valuenow=\"20\" aria-valuemin=\"0\" aria-valuemax=\"100\"></div>",
			"</div>"
		],
		"description": "b5-progress-multiple"
	}
}