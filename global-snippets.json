	"Bootstrap 5 Accordion Open": {
		"prefix": "b5-accordion-open",
		"body": [
			"<div class=\"accordion\" id=\"accordionPanelsStayOpenExample\">",
				"\t<div class=\"accordion-item\">",
					"\t\t<h2 class=\"accordion-header\" id=\"panelsStayOpen-headingOne\">",
						"\t\t\t<button class=\"accordion-button\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#panelsStayOpen-collapseOne\" aria-expanded=\"true\" aria-controls=\"panelsStayOpen-collapseOne\">",
							"\t\t\t\t${1:Accordion Item #1}",
